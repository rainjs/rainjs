{
    "id": "example",
    "version": "3.0",
    "views": {
        "index" : {},
        "notes": {},
        "event_emitter": {
            "view": "event_emitter.html",
            "controller": {
                "client": "event_emitter.js",
                "server": "index.js"
            }
        },
        "level1" : {},
        "level2" : {},
        "level3" : {},
        "css_cross_referencing": {},
        "platform_language": {},
        "layout_localization": {},
        "intent_security": {
            "controller": {
                "client": "intent_security.js"
            }
        },
        "denied": {
            "permissions": ["some_permission"]
        },
        "image_localization": {},
        "text_localization": {},
        "client_side_text_localization": {},
        "media_queries": {},
        "containers_v1": {},
        "containers_v2": {},
        "layout/flow": {}
    },
    "intents": [
        {
            "category": "com.rain.example.security",
            "action": "ALLOWED_VIEW",
            "provider": "level1"
        },
        {
            "category": "com.rain.example.security",
            "action": "DENIED_VIEW",
            "provider": "denied"
        },
        {
            "category": "com.rain.example.security",
            "action": "ALLOWED_SERVER",
            "provider": "index.js#allowedIntent"
        },
        {
            "category": "com.rain.example.security",
            "action": "DENIED_SERVER",
            "permissions": ["some_permission"],
            "provider": "index.js#deniedIntent"
        }
    ]
}
